        <rdfs:label xml:lang="en">descriptive_property</rdfs:label>
    </owl:Class>
    


    <!-- http://emmc.info/emmo-properties#EMMO_dd4a7f3e_ef56_466c_ac1a_d2716b5f87ec -->

    <owl:Class rdf:about="http://emmc.info/emmo-properties#EMMO_dd4a7f3e_ef56_466c_ac1a_d2716b5f87ec">
        <rdfs:subClassOf rdf:resource="http://emmc.info/emmo-properties#EMMO_2a888cdf_ec4a_4ec5_af1c_0343372fc978"/>
        <rdfs:subClassOf rdf:resource="http://emmc.info/emmo-semiotics#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10"/>
        <emmo-core:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>A &apos;property&apos; that can be quantified with respect to a standardized reference physical instance (e.g. the prototype meter bar, the kg prototype) or method (e.g. resilience) through a measurement process.</emmo-core:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <rdfs:label xml:lang="en">quantitative_property</rdfs:label>
    </owl:Class>
    


    <!-- http://emmc.info/emmo-properties#EMMO_f2d5d3ad_2e00_417f_8849_686f3988d929 -->

    <owl:Class rdf:about="http://emmc.info/emmo-properties#EMMO_f2d5d3ad_2e00_417f_8849_686f3988d929">
        <rdfs:subClassOf rdf:resource="http://emmc.info/emmo-properties#EMMO_1b52ee70_121e_4d8d_8419_3f97cd0bd89c"/>
        <rdfs:label xml:lang="en">measurement_instrument</rdfs:label>
    </owl:Class>

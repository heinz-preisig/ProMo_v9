        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://emmc.info/emmo-core#EMMO_85fc27bc_5e42_4baa_bf85_5a2dc4a26a1f"/>
                <owl:allValuesFrom rdf:resource="http://emmc.info/emmo-core#EMMO_d82fd6c6_aebe_440f_9dd2_24a4eb3a417b"/>
